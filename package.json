{
  "name": "order-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:clean": "npm run clean && next dev -p 3001",
    "build": "next build",
    "build:clean": "npm run clean && next build",
    "start": "next start",
    "clean": "node scripts/clean.mjs",
    "lint": "next lint",
    "gate:governance": "npm run lint && npm run security:check:api-route-guards && npm run audit:menu-coverage && npm run audit:permission-flow && npm run perf:menu-visibility && npm run test:unit",
    "gate:staging-docker": "node scripts/ci/run-docker-staging-e2e.mjs",
    "audit:menu-coverage": "node scripts/audit-menu-coverage.mjs",
    "audit:permission-flow": "node scripts/audit-permission-flow-coverage.mjs",
    "perf:menu-visibility": "node scripts/perf-menu-visibility.mjs",
    "perf:stock:phase9": "node scripts/perf-stock-phase9.mjs",
    "validate": "npm run lint && npm run build",
    "test": "npm run test:unit",
    "test:unit": "jest --config jest.config.ts",
    "test:watch": "jest --config jest.config.ts --watch",
    "test:e2e": "playwright test",
    "test:e2e:api": "playwright test --config playwright.api.config.ts",
    "test:e2e:api:docker": "playwright test --config playwright.api.docker.config.ts",
    "test:e2e:docker": "playwright test --config playwright.docker.config.ts",
    "test:e2e:deterministic": "node scripts/run-e2e-deterministic.mjs",
    "security:check:api-route-guards": "node scripts/check-api-route-guards.mjs",
    "test:smoke:api": "jest --config jest.config.ts --runInBand src/__tests__/api/health.route.test.ts",
    "test:smoke:api:all": "jest --config jest.config.ts --runInBand src/__tests__/api/health.route.test.ts src/__tests__/api/pos-orders-summary.route.test.ts src/__tests__/api/pos-dashboard-sales.route.test.ts src/__tests__/api/permissions-users-update.route.test.ts src/__tests__/api/permissions-audits.route.test.ts src/__tests__/api/pos-products-delete.route.test.ts src/__tests__/api/route-error.util.test.ts",
    "test:smoke:resilience": "jest --config jest.config.ts --runInBand src/__tests__/services/orders.service.contract.test.ts src/__tests__/services/dashboard.service.contract.test.ts",
    "test:go-live:phase4": "npm --prefix ../Order-Project-Backend run test:go-live:phase4",
    "test:go-live:phase5": "npm run lint && npm run security:check:api-route-guards && npm run test:smoke:api:all && npm run test:unit -- --runInBand src/__tests__/rbac/page-permission-map.test.ts",
    "test:go-live:phase6": "npm run test:smoke:resilience",
    "test:go-live:phase7": "node scripts/run-go-live-phase7.mjs",
    "test:go-live:phase8": "node scripts/run-go-live-phase8.mjs",
    "test:go-live:phase9": "node scripts/run-go-live-phase9.mjs",
    "test:go-live:phase7:signoff": "node scripts/run-go-live-phase7-signoff.mjs",
    "test:go-live:phase9:signoff": "node scripts/run-go-live-phase9-signoff.mjs",
    "perf:pos:profile": "node scripts/perf-pos-main-before-after.mjs",
    "perf:pos:baseline:assert": "node scripts/assert-perf-pos-baseline.mjs",
    "perf:pos:baseline:ci": "npm run perf:pos:profile && npm run perf:pos:baseline:assert"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@tanstack/react-query": "^5.90.19",
    "@types/qrcode.react": "^1.0.5",
    "antd": "^6.1.2",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "jose": "^6.1.3",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "lodash": "^4.17.21",
    "next": "^14.2.22",
    "postcss": "^8",
    "promptpay-qr": "^0.5.0",
    "qrcode.react": "^4.2.0",
    "react": "^18",
    "react-dom": "^18",
    "socket.io-client": "^4.8.3",
    "tailwindcss": "^3.4.1",
    "xlsx": "^0.18.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.23",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "^14.2.22",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
